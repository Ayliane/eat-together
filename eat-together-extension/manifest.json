{
  "manifest_version": 2,
  "name": "Eat-together",
  "description": "Integration des notes tripadvisor sur foodora et deliveroo",
  "version": "3.0",

  "permissions": [
    "activeTab",
    "https://www.eatogether.uk/"
  ],
  "content_scripts": [{
    "matches": ["*://deliveroo.fr/*", "*://*.foodora.fr/*"],
    "js": ["jquery-3.2.1.min.js", "eat-together.js"],
    "css": ["chrome-extension.css"]
  }]
}
